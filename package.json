{
  "name": "NodeJsWebApp",
  "version": "1.0.0",
  "author": "Josh Bacon",
  "description": "Node.js web app example using express.js framework, mongodb, pug view engine, and various other pacakges.",
  "main": "./bin/www",
  "scripts": {
    "start": "node --inspect=localhost:9229 --inspect-brk ./bin/www"
  },
  "license": "ISC",
  "dependencies": {
    "express": "latest",
    "mongodb": "latest",
    "pug": "latest",
    "winston": "latest",
    "jspm": "latest",
    "passport": "latest",
    "body-parser": "latest",
    "cookie-parser": "latest",
    "express-session": "latest",
    "connect-mongodb-session": "latest",
    "debug": "latest",
    "express-winston": "latest",
    "serve-favicon": "latest",
    "markdown": "latest",
    "sanitize-html": "latest",
    "passport-local": "latest",
    "passport-jwt": "latest",
    "validator": "latest",
    "bcryptjs": "latest"
  },
  "devDependencies": {
    "jspm": "latest"
  },
  "jspm": {
    "directories": {
      "baseURL": "public"
    },
    "dependencies": {
      "cookie": "npm:cookie@^0.3.1",
      "jquery": "npm:jquery@^3.2.1"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
